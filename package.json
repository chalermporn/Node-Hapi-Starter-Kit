{
  "name": "node_hapi_starter_kit",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "NODE_ENV=local_postgres nodemon server.js",
    "lint": "eslint --max-warnings 3 ./",
    "test": "npm run lint && lab -c",

    "migrate": "node_modules/.bin/sequelize db:migrate; node_modules/.bin/sequelize db:seed:all",
    "migrate-localPSQL": "NODE_ENV=local_postgres npm run migrate",
    "migrate-prod": "NODE_ENV=production npm run migrate",

    "drop": "node_modules/.bin/sequelize db:drop",
    "drop-localPSQL": "NODE_ENV=local_postgres npm run drop",
    "drop-prod": "NODE_ENV=production npm run drop",

    "create": "node_modules/.bin/sequelize db:create",
    "create-localPSQL": "NODE_ENV=local_postgres npm run create",
    "create-prod": "NODE_ENV=production npm run create"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahuljain1311/Node-Hapi-Starter-Kit.git"
  },
  "author": "Rahul Jain <rahuljain1311@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "babel-core": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "confidence": "^3.0.2",
    "fs": "^0.0.1-security",
    "glue": "^4.2.0",
    "good": "^6.6.3",
    "good-console": "^5.3.2",
    "hapi": "^16.5.2",
    "hapi-swagger": "^7.7.1",
    "inert": "^4.2.1",
    "joi": "^10.6.0",
    "lodash": "^4.17.4",
    "mysql": "^2.15.0",
    "mysql2": "^1.5.3",
    "path": "^0.12.7",
    "pg": "^7.4.1",
    "sequelize": "^4.37.6",
    "sequelize-cli": "^4.0.0",
    "vision": "^4.1.1"
  },
  "devDependencies": {
    "code": "^4.1.0",
    "lab": "^14.2.2",
    "nodemon": "^1.11.0"
  }
}
